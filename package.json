{
  "name": "world-cup-scores",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && push-dir --dir=dist --branch=gh-pages --allow-unclean --force"
  },
  "dependencies": {
    "axios": "0.18.0",
    "cheerio": "1.0.0-rc.2",
    "lodash": "4.17.21",
    "sw-precache-webpack-plugin": "0.11.5",
    "vue": "2.5.17",
    "vue-moment": "4.0.0",
    "vue-router": "3.0.1",
    "vue-tabs-component": "1.5.0",
    "vuex": "3.0.1",
    "vuex-persistedstate": "2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.1.1",
    "@vue/cli-plugin-eslint": "3.1.4",
    "@vue/cli-service": "3.1.2",
    "gulp": "3.9.1",
    "node-sass": "4.14.1",
    "normalize-scss": "7.0.1",
    "sass-loader": "7.1.0",
    "vue-awesome": "3.2.0",
    "vue-lazyload": "1.2.6",
    "vue-template-compiler": "2.5.17"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "no-unused-vars": 0,
      "no-cond-assign": 0,
      "no-console": 0,
      "no-undef": 0,
      "vue/require-v-for-key": 0,
      "vue/valid-v-for": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
